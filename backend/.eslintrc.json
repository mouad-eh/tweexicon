{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest/globals": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": ["jest"],
    "rules": {
        "no-console": "off",
        "no-underscore-dangle": "off",
        "no-await-in-loop": "off",
        "no-restricted-syntax": "off"
    }
}