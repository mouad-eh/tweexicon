{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/jsx-filename-extension": "off",
        "react/prop-types": "off",
        "max-len": "off",
        "no-underscore-dangle": "off",
        "react/no-unescaped-entities": "off",
        "no-param-reassign": "off",
        "no-unused-vars": "off",
        "react/jsx-props-no-spreading": "off",
        "no-console": "off"
    }
}